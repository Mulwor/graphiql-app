{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 100,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "singleAttributePerLine": true,
  "jsxSingleQuote": true,
  "useTabs": false,
  "trailingComma": "all",
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindConfig": "./tailwind.config.js",
  "endOfLine": "auto"
}
